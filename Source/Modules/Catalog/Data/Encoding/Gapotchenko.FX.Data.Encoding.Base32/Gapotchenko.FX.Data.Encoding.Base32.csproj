    <PackageDescription>Provides Base32 data encoding algorithms: Base32, base32-hex, Crockford Base 32, z-base-32.</PackageDescription>
  </PropertyGroup>

  <ItemGroup>
    <PackageTag Include="base32;base32-hex;crockford-base-32;z-base-32;rfc4648" />
