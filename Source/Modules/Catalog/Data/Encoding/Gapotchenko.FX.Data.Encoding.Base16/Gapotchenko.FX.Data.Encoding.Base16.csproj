<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <CLSCompliant>true</CLSCompliant>
    <PackageDescription>Provides Base16 data encoding algorithms.</PackageDescription>
  </PropertyGroup>

  <ItemGroup>
    <PackageTag Include="base16;rfc4648" />
    <CommonlyUsedPart Include="Gapotchenko.FX.Data.Encoding.Base16" />
  </ItemGroup>

  <PropertyGroup>
    <AnalysisMode>all</AnalysisMode>
    <WarningsAsErrors>$(WarningsAsErrors),CA1307</WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Gapotchenko.FX\Gapotchenko.FX.csproj" />
    <ProjectReference Include="..\Gapotchenko.FX.Data.Encoding\Gapotchenko.FX.Data.Encoding.csproj" />
  </ItemGroup>

</Project>
