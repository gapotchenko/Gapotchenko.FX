<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageDescription>Provides data structures and primitives for working with intervals.</PackageDescription>
  </PropertyGroup>

  <ItemGroup>
    <PackageTag Include="math;interval" />
    <CommonlyUsedPart Include="Gapotchenko.FX.Math.Intervals.Interval&lt;T&gt;" />
    <CommonlyUsedPart Include="Gapotchenko.FX.Math.Intervals.ValueInterval&lt;T&gt;" />
  </ItemGroup>

  <PropertyGroup>
    <NoWarn>$(NoWarn),GPFX0001</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Using Include="System.Numerics" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Gapotchenko.FX.Memory\Gapotchenko.FX.Memory.csproj" />
    <ProjectReference Include="..\Gapotchenko.FX.Math\Gapotchenko.FX.Math.csproj" />
    <ProjectReference Include="..\..\Gapotchenko.FX\Gapotchenko.FX.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
