<Project>

  <PropertyGroup>
    <TargetFrameworks Condition=" '$(MasteringProjectKind)' == 'module' OR '$(MasteringProjectKind)' == 'profile' ">net472;netstandard2.0;netstandard2.1;net6.0;net7.0;net8.0;net9.0;net10.0</TargetFrameworks>
    <TargetFrameworks Condition=" '$(MasteringProjectKind)' == 'test' ">net481;net8.0;net9.0;net10.0</TargetFrameworks>
  </PropertyGroup>

  <!-- ================================================================== -->

  <PropertyGroup>
    <MasteringFrameworkIdentifier>$([MSBuild]::GetTargetFrameworkIdentifier($(TargetFramework)))</MasteringFrameworkIdentifier>
    <MasteringFrameworkVersion>$([MSBuild]::GetTargetFrameworkVersion($(TargetFramework)))</MasteringFrameworkVersion>
  </PropertyGroup>

  <Import Project="TargetFrameworkFeatures.props" />

</Project>
