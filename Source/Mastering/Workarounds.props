<Project>

  <PropertyGroup>
    <DefaultItemExcludes>$(DefaultItemExcludes);**\*.orig;**\launchSettings.json</DefaultItemExcludes>

    <!-- peverify-compat: https://github.com/dotnet/roslyn/issues/22485 -->
    <Features>$(Features);peverify-compat</Features>

    <!-- Bug in .NET SDK 6.0 preview 7 -->
    <DisableImplicitNamespaceImports>true</DisableImplicitNamespaceImports>		
  </PropertyGroup>

</Project>
