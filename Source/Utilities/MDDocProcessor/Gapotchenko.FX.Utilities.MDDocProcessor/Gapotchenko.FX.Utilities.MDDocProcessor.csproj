<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>MDDocProcessor</AssemblyName>
  </PropertyGroup>
  
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <SignAssembly>false</SignAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>$(NoWarn),SYSLIB1045</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Gapotchenko.FX" Version="2025.1.45" />
    <PackageReference Include="Gapotchenko.FX.Math" Version="2025.1.45" />
    <PackageReference Include="Gapotchenko.FX.Math.Graphs" Version="2025.1.45" />
    <PackageReference Include="Gapotchenko.FX.Text" Version="2025.1.45" />
    <PackageReference Include="Markdig.Signed" Version="0.38.0" />
    <PackageReference Include="Mono.Options" Version="6.12.0.148" />
  </ItemGroup>

</Project>
